{% if metafields.custom.specs %}
  <div>
    <strong>Specifications Table:</strong>
    <table style="width:100%;">
      {% assign specs = metafields.custom.specs.value %}
      {% for item in specs %}
        <tr>
          <th>{{ item[0] }}</th>
          <td>{{ item[1] }}</td>
        </tr>
      {% endfor %}
    </table>
  </div>
{% endif %}
